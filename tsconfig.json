{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["components/*"],
      "@api": ["components/api"],
      "@screens/*": ["components/screens/*"],
      "@UI": ["components/UI/"],
      "@config": ["config/"],
      "@hooks/*": ["hooks/*"],
      "@store": ["store/"],
      "@Types/*": ["types/*"],
      "@assets/*": ["assets/*"],
      "@routes": ["routes/"],
    }
  },
}
